        <messages/>
        <name> NeedIt Request Type Options</name>
        <order/>
        <script><![CDATA[function onLoad() {
   var whatNeededField = g_form.getGlideUIElement('what_needed'); 
   
   
   if (!g_form.isNewRecord()) {
     
      whatNeededField.setOriginalValue(whatNeededField.getValue());
   } else {
     
      g_form.clearOptions('what_needed');
      
    
      var requestType = g_form.getValue('request_type'); 
      
      switch (requestType) {
         case 'Human Resources':
            g_form.addOption('what_needed', 'Human Resources 1', 'Human Resources 1');
            g_form.addOption('what_needed', 'Facilities 2', 'Facilities 2');
            g_form.addOption('what_needed', 'Other', 'Other');
            break;
         case 'Facilities':
            g_form.addOption('what_needed', 'Facilities 1', 'Facilities 1');
            g_form.addOption('what_needed', 'Facilities 2', 'Facilities 2');
            g_form.addOption('what_needed', 'Other', 'Other');
            break;
         case 'Legal':
            g_form.addOption('what_needed', 'Legal 1', 'Legal 1');
            g_form.addOption('what_needed', 'Legal 2', 'Legal 2');
            g_form.addOption('what_needed', 'Other', 'Other');
            break;
         default:
            break;
      }
   }
}
]]></script>
